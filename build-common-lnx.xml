<project name="common-linux">
	
	<import file="build-common.xml" />
	
	<target name="batch">
		<echo file="${build.dir}/dist/${project.name.lowercase}-win.sh">java -classpath "${project.name.lowercase}.jar:lib/*" ${main.class.win} $1</echo>
		<echo file="${build.dir}/dist/${project.name.lowercase}-cmd.sh">java -classpath "${project.name.lowercase}.jar:lib/*" ${main.class.cmd} $1 $2</echo>
	</target>
	
</project>