<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>
<project name="common-lnx">
	
	<import file="build-common.xml" />
	
	<target name="batch">
		<echo file="${build.dir}/dist/${project.name.lowercase}-win.sh">"$JAVA_HOME/bin/java" -classpath "${project.name.lowercase}.jar:lib/*" ${main.class.win} $1</echo>
		<echo file="${build.dir}/dist/${project.name.lowercase}-cmd.sh">"$JAVA_HOME/bin/java" -classpath "${project.name.lowercase}.jar:lib/*" ${main.class.cmd} $1 $2</echo>
	</target>
	
</project>
