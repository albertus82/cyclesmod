<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>
<project name="common-win">
	
	<import file="build-common.xml" />
	
	<target name="batch">
		<echo file="${build.dir}/dist/${project.name.lowercase}-win.bat">@start javaw -classpath "%~dp0${project.name.lowercase}.jar;%~dp0lib/*" ${main.class.win} %1</echo>
		<echo file="${build.dir}/dist/${project.name.lowercase}-cmd.bat">@java -classpath "${project.name.lowercase}.jar;lib/*" ${main.class.cmd} %1 %2</echo>
	</target>
	
</project>
